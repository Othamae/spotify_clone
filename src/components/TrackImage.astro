---
import type { Artist2 } from "@/types/types"
interface Props {
  url: string
  name: string
  artists: Artist2[]
}
const { url, name, artists } = Astro.props
const listOfArtist: String[] = []
if (artists) {
  artists.map((artist) => {
    listOfArtist.push(artist.name)
  })
}
---

<picture class="aspect-square w-11 h-11 flex-none">
  <img
    src={url}
    alt={`${name} by ${listOfArtist.join(", ")}`}
    class="object-cover w-11 h-11"
  />
</picture>
